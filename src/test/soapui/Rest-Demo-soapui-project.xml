def response = messageExchange.response.responseContent;
def slurper = new JsonSlurper();
def json = slurper.parseText(response);

log.info json.results.address_components.long_name[0].size();
count = json.results.address_components.long_name[0].size();
longNameContext = json.results.address_components.long_name[0];

for(i in 1..&lt;count)
{
  log.info longNameContext[i];
  if(i == 5)
    assert longNameContext[i] == "United States";
}
